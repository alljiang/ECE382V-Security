diff --git a/docker-database/Dockerfile b/docker-database/Dockerfile
new file mode 100644
index 0000000..6715054
--- /dev/null
+++ b/docker-database/Dockerfile
@@ -0,0 +1,38 @@
+FROM debian:9.2
+
+RUN echo "deb http://archive.debian.org/debian stretch main" > /etc/apt/sources.list
+
+RUN apt-get update && \
+    apt-get upgrade -y && \
+    DEBIAN_FRONTEND=noninteractive apt-get install -y \
+    debconf-utils && \
+    echo mariadb-server mysql-server/root_password password vulnerables | debconf-set-selections && \
+    echo mariadb-server mysql-server/root_password_again password vulnerables | debconf-set-selections && \
+    DEBIAN_FRONTEND=noninteractive apt-get install -y \
+    apache2 \
+    mariadb-server \
+    php \
+    php-mysql \
+    php-pgsql \
+    php-pear \
+    php-gd \
+    net-tools \
+    && \
+    apt-get clean && \
+    rm -rf /var/lib/apt/lists/*
+
+RUN service mysql start && \
+    sleep 3 && \
+    mysql -uroot -pvulnerables -e "CREATE USER app@'%' IDENTIFIED BY 'vulnerables'; \
+	CREATE DATABASE dvwa; \
+	GRANT ALL privileges ON dvwa.* TO 'app'@'%'; \
+	"
+
+RUN echo "[mysqld] \n \
+skip-networking=0 \n \
+skip-bind-address" >> /etc/mysql/mariadb.conf.d/50-server.cnf
+
+COPY main_database.sh /
+ENTRYPOINT ["/main_database.sh"]
+
+EXPOSE 3306 33060
